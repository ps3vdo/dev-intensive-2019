<templateSet group="user">
  <template name="Ld" value="Log.d(&quot;$className$&quot;,&quot;$END$&quot;)" description="Log.d(TAG, message)" toReformat="false" toShortenFQNames="true">
    <variable name="className" expression="fileNameWithoutExtension()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="KOTLIN" value="true" />
    </context>
  </template>
</templateSet>